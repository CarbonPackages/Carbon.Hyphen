{
    "name": "carbon/hyphen",
    "type": "neos-carbon",
    "description": "Make hyphens easier in Neos CMS",
    "require": {
        "neos/neos": "~3.0 || ~4.0"
    },
    "autoload": {
        "psr-4": {
            "Carbon\\Hyphen\\": "Classes"
        }
    },
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://jonnitto.ch",
            "role": "Developer"
        }
    ],
    "archive": {
        "exclude": [".editorconfig", ".gitattributes", ".github", ".gitignore", "CODE_OF_CONDUCT.md"]
    },
    "extra": {
        "installer-name": "Carbon.Hyphen",
        "neos": {
            "package-key": "Carbon.Hyphen"
        }
    }
}
